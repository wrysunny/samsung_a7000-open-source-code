<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <!-- <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" > -->

            <LinearLayout
                android:id="@+id/layout_rssi"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_rssi"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/RSSI" >
                </TextView>

                <EditText
                    android:id="@+id/rssi"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_snr"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_snr"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/SNR" >
                </TextView>

                <EditText
                    android:id="@+id/snr"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_cnt"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_cnt"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/CNT" >
                </TextView>

                <EditText
                    android:id="@+id/cnt"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_rssi2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_rssi_2"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/RSSI" >
                </TextView>

                <EditText
                    android:id="@+id/rssi_2"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_snr2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_snr_2"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/SNR" >
                </TextView>

                <EditText
                    android:id="@+id/snr_2"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout_cnt2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_cnt_2"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/CNT" >
                </TextView>

                <EditText
                    android:id="@+id/cnt_2"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_af"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/AF_Threshold" >
                </TextView>

                <EditText
                    android:id="@+id/af_threshold"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_af_valid"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/AF_Valid_Threshold" >
                </TextView>

                <EditText
                    android:id="@+id/af_valid_threshold"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_current"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/Current_Threshold" >
                </TextView>

                <EditText
                    android:id="@+id/current_threshold"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:editable="false"
                    android:focusable="false"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/text_currentSNR"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center" >
                </TextView>

                <EditText
                    android:id="@+id/current_snr"
                    android:layout_width="150dp"
                    android:layout_height="match_parent"
                    android:editable="false"
                    android:focusable="false"
                    android:inputType="numberSigned" >
                </EditText>
            </LinearLayout>
 
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/okbtn"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" >
                </Button>

                <Button
                    android:id="@+id/cancelbtn"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content" >
                </Button>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/skiptedtunningvalue"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" >
                </Button>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/thailandapplybtn"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" >
                </Button>

                <Button
                    android:id="@+id/thailandcancelbtn"
                    android:layout_width="160dp"
                    android:layout_height="wrap_content" >
                </Button>
            </LinearLayout>
        </LinearLayout>
    <!-- </LinearLayout> -->

</ScrollView>
